#navbar
  position relative // as opposed to relative, fixed, etcs
  margin-bottom 50px
  top 0
  left 0
  width calc(100% - 40px)
  padding 0px
  background: none

#navbar a 
    text-decoration none

body
    margin 0
    padding 0
    font-family "Roboto", sans-serif
    color white
    background-image url("http://static.hdw.eweb4.com/media/wallpapers_dl/1/148/1474890-neon-robots.jpg")

    
.error-message
    visibility hidden
    color red
    font-weight bold
    text-align left
    padding 1rem 0
.modal
    display none
    position fixed
    left 0
    top 0
    z-index 100
    width 100%
    height 100%
    overflow auto
    background-color #000
    background-color rgba(0, 0, 0, .5)
.modal-content
    max-width 90%
    margin 10% auto
    padding 2rem
    border 1px solid #888
    background-color white
    border-radius 5px
    text-align center

